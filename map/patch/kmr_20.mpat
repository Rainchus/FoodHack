#new:Function $GivePlayerStartingEquipment {
	LIO t0, 8010F290
	ORI t1, r0, 1
	SB r0, 0001 (t0) % sets the Hammer
	SB t1, 000A (t0) % sets the AC
	JR RA
	ORI v0, r0, 2
  }

@ $Script_8024FD70
{
	Call $GivePlayerStartingEquipment
	Set *GB_StoryProgress 90
	Call  GotoMap ( "jam_00" 0 )
  Return
  End
}
